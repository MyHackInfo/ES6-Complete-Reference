<body>
  <script type="text/javascript">

  // IN This Array Show Two Method Array.of() and Array.from()
  // 1 this is normal array
  // in this one 1 payameter mean length
  items=new Array(2,4);
  console.log(items.length);
  console.log(items[0]);
  console.log(items[1]);

  // 2 Array.of() method
  // but in this one parameter mean array value
  items=Array.of(8 ,"this is");
  console.log(items.length);
  console.log(items[0]);
  console.log(items[1]);

  // 3 Object conver into array usnig Array.from() method
    let number={
      *[Symbol.iterator](){
        yield 1;
        yield 2;
        yield 3;
        yield 4;
      }
    }
    let number2 = Array.from(number,(value) => value +5);
    console.log(number2);

  </script>

</body>
