<body>
  <script type="text/javascript">

  // Array New Method
  let numbers=[12,3,45,65,77,8];
  console.log(numbers.find(n => n =>12));  // this is find() Method
  console.log(numbers.findIndex(n => n=>12)); // this is findIndex() Method

    // fill() method value take (value, startinde, endindex)
    numbers.fill(2);                       // this is fill() Method
    console.log(numbers.toString());

    numbers.fill(0 , 3 ,5);
    console.log(numbers.toString());

    numbers.copyWithin(3,0,4);
    console.log(numbers.toString());


  </script>

</body>
